.seriestips {
    visibility: hidden;
    width: 120px;
    background-color: black;
    color: #fff;
    text-align: center;
    border-radius: 6px;
    padding: 5px 0;
    opacity: 0.5
    /* Position the tooltip */
    position: absolute;
    z-index: 11;
}
.fa.fa-download:hover .seriestips {
    visibility: visible;
}

