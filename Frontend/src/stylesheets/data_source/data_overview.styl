.dtrg-group
    background-color #f9f9f9

ul.group-breadcrumb 
  list-style none
  margin-bottom 0
ul.group-breadcrumb  li 
  display inline

ul.group-breadcrumb  li+li:before 
  color black
  content "/\00a0"

ul.group-breadcrumb  li a 
  color #0275d8
  text-decoration none

ul.group-breadcrumb  li a:hover 
  color #01447e
  cursor pointer
  text-decoration underline

table.dataTable tbody tr.selected
    background-color #B0BED9

.filesDownloadProgressDisplay
	color #8a6d3b
	background-color #fcf8e3
	border-color #faebcc
	padding 15px
	border 1px solid transparent
	border-radius 4px