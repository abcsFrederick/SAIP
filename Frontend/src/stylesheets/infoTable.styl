#infoList.table-fixed
	width 450px
	
	tbody,thead,tr 
		display block
	tbody
		height 250px
		overflow-y auto
		overflow-x hidden
		
.fa
	cursor pointer
	color darkslategray
img
	width 33px
	padding 1px
	border 0px solid #021a40
	background-color white
	
img:hover
	transform scale(3)

.eye-red
	color red

.left
	display inline-block
	width 17px
	
.slicetips {
    visibility: hidden;
    width: 120px;
    background-color: black;
    color: #fff;
    text-align: center;
    border-radius: 6px;
    padding: 5px 0;
    opacity: 0.5
    /* Position the tooltip */
    position: absolute;
    z-index: 11;
}
.fa.fa-download:hover .slicetips {
    visibility: visible;
}